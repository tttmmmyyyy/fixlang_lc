{
    "name": "fixlang-mode",
    "version": "0.0.1",
    "author": "tttmmmyyyy",
    "license": "MIT",
    "description": "A language server client for fixlang",
    "engines": {
        "vscode": "^1.92.0"
    },
    "main": "./extension.js",
    "activationEvents": [
        "onLanguage:fixlang"
    ],
    "contributes": {
        "languages": [
            {
                "id": "fixlang",
                "aliases": [
                    "fixlang",
                    "fix-lang"
                ],
                "extensions": [
                    ".fix"
                ],
                "filenames": [
                    "fixproj.toml"
                ]
            }
        ]
    },
    "dependencies": {
        "vscode-languageclient": "^9.0.1"
    },
    "private": true
}