{
    "name": "fixlang-language-client",
    "displayName": "Fix-lang language client",
    "version": "0.0.1",
    "license": "MIT",
    "description": "A language server client for fixlang",
    "repository": {
        "type": "git",
        "url": "https://github.com/tttmmmyyyy/fixlang_lc.git"
    },
    "icon": "icon.png",
    "categories": [
        "Programming Languages"
    ],
    "engines": {
        "vscode": "^1.92.0"
    },
    "main": "./extension.js",
    "activationEvents": [
        "onLanguage:fixlang"
    ],
    "contributes": {
        "languages": [
            {
                "id": "fixlang",
                "aliases": [
                    "fixlang",
                    "fix-lang"
                ],
                "extensions": [
                    ".fix"
                ],
                "filenames": [
                    "fixproj.toml"
                ]
            }
        ]
    },
    "dependencies": {
        "vscode-languageclient": "^9.0.1"
    },
    "publisher": "tttmmmyyyy"
}